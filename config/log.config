[loggers]
keys=root

[handlers]
keys=hand

[formatters]
keys=fmt

[logger_root]
level=DEBUG
handlers=hand

[handler_hand]
class=StreamHandler
level=INFO
formatter=fmt
args=(sys.stdout,)

[formatter_fmt]
format=%(name)s,%(threadName)s,%(created)f,%(levelname)s,%(message)s
datefmt=
class=logging.Formatter

